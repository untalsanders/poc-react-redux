{
    "name": "poc-react-redux",
    "private": "true",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run api:fake\" \"vite --mode development\"",
        "start": "vite --mode development",
        "build": "tsc && vite build --mode production",
        "preview": "vite preview",
        "format:check": "prettier . --check",
        "format": "prettier . --write",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "api:fake": "npx json-server db.json"
    },
    "devDependencies": {
        "@eslint/js": "9.15.0",
        "@types/react": "18.3.12",
        "@types/react-dom": "18.3.1",
        "@vitejs/plugin-react-swc": "3.7.2",
        "concurrently": "9.1.0",
        "eslint": "9.15.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.2.1",
        "globals": "15.12.0",
        "json-server": "1.0.0-beta.3",
        "prettier": "3.4.1",
        "typescript": "5.7.2",
        "typescript-eslint": "8.16.0",
        "vite": "6.0.0"
    },
    "dependencies": {
        "@reduxjs/toolkit": "2.3.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-redux": "9.1.2"
    }
}
